{{ ansible_managed | comment }}

DEBUG="false"

KEYCLOAK_URL=https://{{keycloak_service_name}}
KEYCLOAK_REALM={{keycloak_permissions_sync_keycloak_realm}}
KEYCLOAK_SECRET={{keycloak_permissions_sync_keycloak_client_secret}}
KEYCLOAK_CLIENT_ID={{keycloak_permissions_sync_keycloak_client_id}}
KEYCLOAK_REQUIRED_ACTIONS={{ keycloak_required_user_actions | to_json }}

SLEEP_MINUTES=1

KEYCLOAK_AUTHZ_CLIENT="realm-management"
KEYCLOAK_DECISION_STRATEGY="AFFIRMATIVE"
KEYCLOAK_POLICY_ENFORCEMENT_MODE="ENFORCING"

SYNC_IGNORE_GROUPS={{ keycloak_permissions_sync_ignore_groups | to_json }}
SYNC_IGNORE_USERS={{ keycloak_permissions_sync_ignore_users | to_json }}
