---
version: "3"

networks:
  proxy:
    external: true

services:

  {{project_name}}_app:
    container_name: {{project_name}}
    image: "{{collabora_domain_image}}"
    restart: unless-stopped
    logging:
      options:
        max-size: 5m
    networks:
      - proxy
    # ports:
    #  - "{{app_port}}:{{app_port}}"
    #  - "[::]:{{app_port}}:{{app_port}}"
    env_file: 
      - "./env"
    # privileged: true
    cap_add:
      - MKNOD
      # - SYS_CHROOT
      # - FOWNER
